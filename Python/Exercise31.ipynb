   "source": [
    "Create a random 3x3 list of lists and replace all values greater than 0.5 with 1 and all others with 0."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "78aa6f25",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[1, 1, 0], [1, 1, 1], [1, 1, 0]]\n"
     ]
    }
   ],
   "source": [
    "import random\n",
    "\n",
    "matrix = [[random.random() for _ in range(3)]for _ in range(3)]\n",
    "\n",
    "conditions = [[1 if x > 0.5 else 0 for x in row] for row in matrix]\n",
    "\n",
    "print(conditions)"
   ]
  }
